<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="M3Proj.SiteMaster" %>

<!DOCTYPE html>

<html lang="en">
<head runat="server">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="CSS/masterstyle.css" />
    <title><%: Page.Title %> - Kharina Secondary</title>
    <link rel="preconnect" href="https://fonts.googleapis.com"/>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin/>
   
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;500;700;800&family=Playfair+Display:wght@400;500;600;700;800&display=swap" rel="stylesheet">
  
    
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="CSS/footer.css" rel="stylesheet" />
      <asp:ContentPlaceHolder ID="ContentPlaceHolder" runat="server">
            <link rel ="stylesheet" href="teacherstyle.css" />
          
          
          </asp:ContentPlaceHolder>
    

</head>
<body>
    <form runat="server">
        

        <div class="navbox">
            
                <div class="navlogo">

                       <div>
                           <a href="~/"> <img runat="server" src="Photos/download-removebg-preview.png" /> </a> 
                       </div> 
                        <a class="navbar-brand" runat="server" href="~/">Kharina Secondary School</a>
                </div>
                <div class="navlinks">
                    <ul class="navleft">
                        <li><a runat="server" href="~/">Home</a></li>
                        <li><a runat="server" href="~/About.aspx">About</a></li>
                        <li><a runat="server" href="~/Contact.aspx">Contact</a></li>
                        <li class="nav-item dropdown" id="dropdown" runat ="server">
                            <a class=" nav-link dropdown-toggle" data-toggle="dropdown" href="#">Students<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a runat="server" href="~/NewFolder1/Report.aspx">Payments</a></li>
                                <li><a runat="server" href="~/NewFolder1/Fees.aspx">Fees</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown" id="dropdown1" runat ="server">
                             <a class=" nav-link dropdown-toggle" data-toggle="dropdown" href="#">Teacher<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a runat="server" href="~/Teacher.aspx">Students</a></li>
                                <li><a runat="server" href="~/NewFolder1/Subjects.aspx">Subjects</a></li>
                            </ul>
                        </li>
                    </ul>
                    <asp:LoginView runat="server" ViewStateMode="Disabled">
                        <AnonymousTemplate>
                            <ul class="navright">
                                <li><a runat="server" href="~/Account/Register.aspx">Register</a></li>
                                <li>
                                    <asp:Button ID="Button1" CssClass="button" runat="server" Text="Log in" OnClick="Button1_Click" /></li>
                            </ul>
                        </AnonymousTemplate>
                        <LoggedInTemplate>
                            <ul class="nav navbar-nav navbar-right">
                                <li><a runat="server" href="~/Account/Manage" title="Manage your account">Hello <%: Context.User.Identity.GetUserName()  %> !</a></li>
                                <li>
                                    <asp:LoginStatus runat="server" LogoutAction="Redirect" LogoutText="Log off" LogoutPageUrl="~/" OnLoggingOut="Unnamed_LoggingOut" />
                                </li>
                            </ul>
                        </LoggedInTemplate>
                    </asp:LoginView>
                </div>
            </div>
       
        <div class="container body-content">
            <asp:ContentPlaceHolder ID="MainContent" runat="server">
            </asp:ContentPlaceHolder>

        </div>

        <footer>
                <div class="footer_area">
                    <!-- logo -->
                    <div class="box_size">
                        <div class="school_logo">
                            <a href="~/"> <img runat="server" src="Photos/download-removebg-preview.png" /> </a> 
                        </div>
                    </div>

                    
                    <!-- menu -->
                    <div class="box_size">
                        <div>Menu:</div>
                        <div class="menu">
                            <ul>
                                <li><a href="About.aspx">About Us</a></li>
                                <li><a href="Conact.aspx">Contact Us</a></li>
                                <li><a href="">Log In</a></li>
                            </ul>
                        </div>
                    </div>
                    
                    <div class="box_size">
                        <div>Follow Us:</div>
                            <div class="follow">
                                <li><a href="">Instagram</a></li>
                                <li><a href="">Facebook</a></li>
                                <li><a href="">Twitter</a></li>
                            </div>
                    </div>
                </div>
                <p class="copyright">&copy; <%: DateTime.Now.Year %> - My ASP.NET Application</p>
            </footer>
    </form>
</body>
</html>
